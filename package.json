{
  "name": "lumi",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "db:start": "docker compose up",
    "db:push": "bunx drizzle-kit push",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:generate": "bunx @better-auth/cli@latest generate --config ./src/lib/server/auth/index.ts --output ./src/lib/server/db/auth-schema.ts --y && bunx drizzle-kit generate",
    "db:studio": "bunx drizzle-kit studio"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@keyv/sqlite": "^4.0.1",
    "@trpc/client": "^11.0.0-rc.833",
    "@trpc/server": "^11.0.0-rc.833",
    "@types/node": "^22.13.10",
    "better-auth": "^1.2.4",
    "bits-ui": "^1.3.13",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.1",
    "drizzle-valibot": "^0.4.0",
    "keyv": "^5.3.2",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "valibot": "^1.0.0-rc.4"
  }
}

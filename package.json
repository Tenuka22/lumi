{
  "name": "lumi",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "server": "bun run ./src/server.ts",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "db:start": "docker compose up",
    "db:push": "bunx drizzle-kit push",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:generate": "bunx @better-auth/cli@latest generate --config ./src/lib/server/auth/index.ts --output ./src/lib/db/auth-schema.ts --y && bunx drizzle-kit generate",
    "db:studio": "bunx drizzle-kit studio"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@internationalized/date": "^3.7.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "drizzle-kit": "^0.30.5",
    "embla-carousel-svelte": "^8.5.2",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^3.0.0",
    "formsnap": "^2.0.0",
    "globals": "^16.0.0",
    "mode-watcher": "^0.5.1",
    "paneforge": "^1.0.0-next.4",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.24.0",
    "tailwindcss": "^4.0.0",
    "tw-animate-css": "^1.2.4",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vaul-svelte": "^1.0.0-next.6",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@hono/trpc-server": "^0.3.4",
    "@keyv/sqlite": "^4.0.1",
    "@lucide/svelte": "^0.483.0",
    "@tanstack/svelte-query": "^5.69.0",
    "@tanstack/table-core": "^8.21.2",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/node": "^22.13.10",
    "better-auth": "^1.2.4",
    "bits-ui": "^1.3.13",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.1",
    "drizzle-valibot": "^0.4.0",
    "effect": "^3.13.12",
    "hono": "^4.7.5",
    "hono-pino": "^0.8.0",
    "keyv": "^5.3.2",
    "pino": "^9.6.0",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss-animate": "^1.0.7",
    "valibot": "^1.0.0"
  }
}
